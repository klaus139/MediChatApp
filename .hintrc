{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "width: fit-content",
          "height: fit-content"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 106",
    "not chrome <= 107",
    "not edge <= 106",
    "not ios_saf <= 16.1",
    "not opera <= 91",
    "not safari <= 16.1",
    "not samsung <= 18"
  ]
}